        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Squrreltest2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock Text="nincs" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,-40,0,0" FontSize="20" x:Name="version_tbck"/>
        <Button HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,40,0,0" Content="Check" FontSize="20" Click="Button_Click"/>
        <Button HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,150,0,0" Content="Update" FontSize="20" Click="Button_Click_1" x:Name="update_btn" IsEnabled="False"/>

    </Grid>
</Window>
